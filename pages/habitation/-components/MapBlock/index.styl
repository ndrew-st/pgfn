.habitation-map
  margin 0 -18px
  margin-top 30px

  &__content
    width 100%
    height 200px

@media (min-width 768px)
  .habitation-map
    &__content
      height 400px
      max-width 880px

@media (min-width 1280px)
  .habitation-map
    margin 0
    margin-top 30px

    &__content
      max-width 1280px
      height 440px
