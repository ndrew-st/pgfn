body,p,h1,h2,h3,h4,h5,span,b,button,div
  font-family Calibri, "Comic Sans MS", cursive, sans-serif
  margin 0
  padding 0

a
  text-decoration none

body, html
  max-width 100%
  width 100%
  height 100%
  overflow-x hidden

.visually-hidden
  position absolute !important
  clip rect(1px, 1px, 1px, 1px)
  padding 0 !important
  border 0 !important
  height 1px !important
  width 1px !important
  overflow hidden
