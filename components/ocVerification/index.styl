.box
    width: 500px;
    position: absolute
    left calc(50% - 314px)
    padding 64px
    background #FFFFFF
    border-radius: 4px;

.signin
    height: 256px;
    top calc(50% - 192px)

.signup
    height: 296px
    top calc(50% - 212px)

.flex
    display: flex

.h
    font-family: Airbnb Cereal App;
    font-style: normal;
    font-weight: 800;
    font-size: 26px;
    line-height: 36px;
    text-align: left;
    letter-spacing: 0.371429px;
    color: #272528;

.headline
    font-family: Airbnb Cereal App;
    font-style: normal;
    font-weight: 800;
    font-size: 26px;
    line-height: 36px;
    text-align: left;
    letter-spacing: 0.371429px;
    color: #5604C1

.p, .p456, .p15
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 18px;
    line-height: 28px;
    letter-spacing: 0.1px;
    color: #272528;
    margin-top: 8px;
    margin-bottom: 44px;

.p14
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 18px;
    line-height: 28px;
    letter-spacing: 0.1px;
    color: #272528;
    margin-top: 8px;
    margin-bottom: 0;

.p-sms
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 18px;
    line-height: 28px;
    letter-spacing: 0.1px;
    color: #272528;
    margin-top: 8px;

.timer
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 22px;
    line-height: 28px;
    letter-spacing: 0.1px;
    color: #272528;
    margin-top 52px

.lic
    display: inline-block;
    margin-top 36px
    color#989BA8
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 20px;
    letter-spacing: 0.1px;

.code, .code14
    width: 160px
    height 56px
    margin-top 36px
    border none
    outline: none
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 28px;
    line-height: 32px;
    letter-spacing: 9px;
    color: #272528;

.username
    padding: 14px 16px
    width 202px
    height 28px
    background: #F3F3F3;
    box-shadow: inset 0px -1px 0px #D5D1DB;
    border-radius: 4px 4px 0px 0px;
    border none
    outline: none
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 18px;
    line-height: 28px;
    letter-spacing: 0.1px;
    color: #272528;
    transition: box-shadow .2s

.userpassword
    padding: 14px 16px
    width 202px
    height 28px
    margin-left 34px
    background: #F3F3F3;
    box-shadow: inset 0px -1px 0px #D5D1DB;
    border-radius: 4px 4px 0px 0px;
    border none
    outline: none
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 18px;
    line-height: 28px;
    letter-spacing: 0.1px;
    color: #272528;
    transition: box-shadow .2s

.username:focus, .userpassword:focus
    box-shadow: inset 0px -2px 0px #7140B8;

.btn13
    width 160px
    height 56px
    padding: 17px 0 15px 0
    position absolute
    top 332px
    left 404px
    background-color #DFE1E5
    border-radius: 4px;
    border: none;
    font-family: Calibri;
    font-style: normal;
    font-weight: bold;
    font-size: 16px;
    line-height: 24px;
    text-align: center;
    letter-spacing: 0.1px;
    color: #272528;
    transition: background-color .2s

.active
    color #ffffff
    background-color #5604C1;

.enter-top
    display: none;

.enter-bottom
    display: inline-block;
    margin-top: 36px;
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 24px;
    letter-spacing: 0.1px;
    color: #5604C1;

.send-again-timer9
    position: absolute
    top 280px
    left 64px
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 24px;
    letter-spacing: 0.1px;
    color: #5604C1;

.send-again-timer11
    position: absolute
    top 296px
    left 64px
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 24px;
    letter-spacing: 0.1px;
    color: #5604C1;

.send-again-link
    position: absolute
    top 296px
    left 64px
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 20px;
    letter-spacing: 0.1px;
    color: #5604C1;

.goback28, .goback48
    position: absolute
    top 348px
    left 64px
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 24px;
    letter-spacing: 0.1px;
    color: #5604C1;

.old-code
    position: absolute
    top 272px
    left 64px
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 20px;
    letter-spacing: 0.1px;
    color: #E05050;

.reg
    position: absolute;
    left 64px
    bottom 52px
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 24px;
    letter-spacing: 0.1px;
    color: #5604C1;

.reset
    position: absolute;
    left 250px
    bottom 52px
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 24px;
    letter-spacing: 0.1px;
    color: #5604C1;

.copyright
    position: absolute;
    width: 138px;
    height: 20px;
    right: 96px;
    bottom: 31px;
    font-family: Calibri;
    font-style: normal;
    font-weight: normal;
    font-size: 14px;
    line-height: 20px;
    text-align: right;
    letter-spacing: 0.1px;
    color: #C1C4CD;
    padding: 0

@media (max-width: 750px)
    .all
        background-color: #000000
    .box
        width 288px
        height 100%
        padding: 16px
        left: calc(50% - 160px)
    .flex
        display: block
    .p456
        margin-bottom 33px
    .p15
        margin-bottom 60px
    .code
        margin-top 55px
    .code14
        margin-top 31px
    .username
        width 256px
    .userpassword
        margin-left 0
        margin-top 40px
        width: 256px
    .btn13
        position: static
        width 288px
        margin-top 49px
    .reg
        position static
        display: inline-block
        text-align: center
        width 100%
        margin-top 32px
    .reg7
        margin-top 133px
    .reset
        position: static;
        display inline-block
        text-align: center
        margin-top 16px
        width 100%
    .enter-bottom
        display: none
    .enter-top
        display: inline-block
        text-align: center
        width: 100%
        margin-top 32px
    .send-again-timer9
        position: static
        display: inline-block
        margin-top 89px
    .send-again-timer11
        position: static
        display: inline-block
        margin-top 4px
    .send-again-link
        position: static
        display: inline-block
        margin-top 4px
    .old-code
        position: static
        display: inline-block
        margin-top 65px
    .goback28, .goback48
        position: static
        display: inline-block
        text-align: center
        width 100%
    .goback28
        margin-top 28px
    .goback48
        margin-top 48px
    .copyright
        text-align: center
        right: 0
        left: calc(50% - 69px)
        bottom: 22px
